{
  "expo": {
    "name": "NutriGuideBuddy-Mobile",
    "slug": "NutriGuideBuddy-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "nutri-guide-buddy",
    "experiments": {
      "typedRoutes": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nutriguidebuddy.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.NutriGuideBuddyMobile",
      "permissions": ["android.permission.CAMERA"]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-secure-store", "expo-barcode-scanner", "expo-web-browser"],
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "router": {}
    }
  }
}
